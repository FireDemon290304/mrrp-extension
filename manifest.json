{
    "manifest_version": 3,
    "name": ":3 Sound Player",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_title": "Click to enable sound"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }],
    "web_accessible_resources": [{
      "resources": ["Mrrp.mp3"],
      "matches": ["<all_urls>"]
    }]
  }